<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.FinalProject.UMS.StudentManagementMenuController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>


    <Label fx:id="welcomeText" text="This is page 1" />
    <Button onAction="#onOpenProfileButtonClick" text="View/Edit Profile" />
    <Button onAction="#onOpenCoursesButtonClick" text="View Enrolled Courses" />
    <Button onAction="#onOpenGradesButtonClick" text="View Grades" />
    <Button onAction="#onOpenTuitionButtonClick" text="View Tuition Status" />
    <Button onAction="#onOpenSecondPageButtonClick" text="Open Student Info" />
    <Button onAction="#onOpenAdminDashboardButtonClick" text="Admin Dashboard" />
    <Button text="Back to Main Screen" fx:id="backButton" onAction="#handleBackButton" style="-fx-font-size: 14;"/>

</VBox>

